{
  "name": "kowka-boilerplate",
  "description": "Boilerplate to quickstart projects using Gulp and BEM methodology",
  "version": "1.0.0",
  "author": "Gustavo Porto",
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/portothree/kowka-boilerplate.git"
  },
  "homepage": "https://github.com/portothree/kowka-boilerplate",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "task": "gulp",
    "start": "cross-env WATCH=true gulp",
    "clean": "cross-env CLEAN=true gulp compile:templates",
    "build": "cross-env NODE_ENV=production gulp",
    "add": "node ./core/.add",
    "zip": "gulp zip",
    "zip:dev": "cross-env SNAPSHOT=true gulp zip",
    "deploy": "git push origin :gh-pages && git subtree push --prefix dist origin gh-pages"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "autoprefixer": "^9.6.1",
