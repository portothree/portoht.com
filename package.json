    "gulp-svg-sprite": "^1.5.0",
    "gulp-twig": "^1.2.0",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^5.0.0",
    "htmlparser2": "^4.0.0",
    "js-beautify": "^1.10.2",
    "plugin-error": "^1.0.1",
    "postcss-sprites": "^4.2.1",
    "postcss-url": "^8.0.0",
    "stylefmt": "^6.0.3",
    "through2": "^3.0.1"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=5.0.0"
  }
